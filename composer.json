{
    "name": "helsingborg-stad/modularity-products",
    "description": "An addon for displaying products.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Freya Liljeberg",
            "email": "freya.liljeberg@helsingborg.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "require": {
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "ModularityProducts\\": "source/php/"
        }
    },
    "version": "4.0.0",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
